      </geometry>
      <material name="red"/>
    </visual>
  </link>



  <!-- Gazebo customization -->
  <gazebo reference="EE_FORCETORQUESENSOR">
    <kp>1000000.0</kp>
    <kd>1.5</kd>
    <mu1>1000000000</mu1>
    <mu2>1000000000</mu2>
    <self_collide>1</self_collide>
  </gazebo>
  <gazebo reference="EE_FORCETORQUESENSOR_JOINT">
    <provideFeedback>1</provideFeedback>
  </gazebo>
  <link name="EE_IMU"/>
  <joint name="EE_IMU_JOINT" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="EE_FORCETORQUESENSOR"/>
    <child link="EE_IMU"/>
  </joint>

<!-- base -->




  <!-- Base link is the center of the robot's bottom plate -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://mabi_urdf/meshes/base_simplified.stl"/>
      </geometry>
      <material name="gray">
        <color rgba="0.4 0.4 0.4 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.299"/>
      <geometry>
        <box size="0.8 0.7 0.02"/>
      </geometry>
    </collision>
    <collision>
      <origin rpy="0 0 0" xyz="0.39 0 0.1445"/>
      <geometry>
        <box size="0.02 0.7 0.289"/>
      </geometry>
    </collision>
    <collision>
      <origin rpy="0 0 0" xyz="-0.39 0 0.1445"/>
      <geometry>
        <box size="0.02 0.7 0.289"/>
      </geometry>
    </collision>
    <collision>
      <origin rpy="0 0 0" xyz="0 0.34 0.1445"/>
      <geometry>
        <box size="0.76 0.02 0.289"/>
      </geometry>
    </collision>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.34 0.1445"/>
      <geometry>
        <box size="0.76 0.02 0.289"/>
      </geometry>
    </collision>
    <self_filter>
      <origin rpy="0 0 0" xyz="0 0 0.1545"/>
      <geometry>
        <box size="0.8 0.7 0.309"/>
      </geometry>
    </self_filter>
  </link>
  <!-- Base footprint is on the ground plate of the robot -->
  <link name="base_footprint"/>
  <joint name="base_footprint_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 -0.05"/>
    <parent link="base_link"/>
    <child link="base_footprint"/>
  </joint>
  <!-- Interial link stores the robot's inertial information -->
  <link name="base_link_inertia">
    <inertial>
      <mass value="60"/>
      <!-- Update inertias -->
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="2.927405" ixy="0.0" ixz="0.0" iyy="3.677405" iyz="0.0" izz="5.6274"/>
    </inertial>
  </link>
  <joint name="inertial_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.1545"/>
    <parent link="base_link"/>
    <child link="base_link_inertia"/>
  </joint>

  <link name="arm_base">
    <inertial>
      <mass value="0.0001"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="base_to_arm_base" type="fixed">
    <child link="arm_base"/>
    <parent link="base_link"/>
    <origin rpy="0 0 1.57079632679" xyz="0 0 0.309"/>
  </joint>
  <!-- Attaching additional sensors and frames to the base -->
  <!-- IMU Link -->
  <link name="imu_link"/>
  <joint name="imu_joint" type="fixed">
    <origin rpy="3.14159265359 0 3.14159265359" xyz="0.20 0 0.1351"/>
    <parent link="base_link"/>
    <child link="imu_link"/>
  </joint>
  <link name="camera_mount"/>
  <joint name="base_to_camera_mount" type="fixed">
    <child link="camera_mount"/>
    <parent link="base_link"/>
    <origin rpy="3.14159265359 -1.5707963268 0" xyz="-0.38745 -0.00911 0.32995"/>
  </joint>
  <link name="camera_vr_orientation"/>
  <joint name="camera_ros_to_vr_convention" type="fixed">
    <child link="camera_vr_orientation"/>
    <parent link="camera_mount"/>
    <origin rpy="1.5707963268 0 -1.5707963268" xyz="0 0 0"/>
  </joint>
  <link name="realsense_odom_input_frame"/>
  <joint name="base_to_realsense_odom_input_frame" type="fixed">
    <child link="realsense_odom_input_frame"/>
    <parent link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.1"/>
  </joint>
</robot>
